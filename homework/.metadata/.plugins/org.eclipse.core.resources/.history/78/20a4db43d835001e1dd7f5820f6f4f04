package burger_ham;

public class Side {
	String name;
	int cnt;
	
	
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public int getCnt() {
		return cnt;
	}
	public void setCnt(int cnt) {
		this.cnt = cnt;
	}
	@Override
	public String toString() {
		return "Side [name=" + name + ", cnt=" + cnt + "]";
	}
	
	
	
}
