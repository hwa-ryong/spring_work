package di_p;

public class Wheel {
	String name;
	int a = 2;
	int b = 3;
	
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public int getA() {
		return a;
	}
	public void setA(int a) {
		this.a = a;
	}
	public int getB() {
		return b;
	}
	public void setB(int b) {
		this.b = b;
	}
	@Override
	public String toString() {
		return "wheel [name=" + name + ", a=" + a + ", b=" + b + "]";
	}
	
	
	
	
}
